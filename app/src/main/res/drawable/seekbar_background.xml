<?xml version="1.0" encoding="utf-8"?><!-- seekbar_progress.xml -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">

        <shape>
            <solid android:color="#262E3B" /> <!-- 进度背景颜色 -->
            <corners android:radius="8px" /> <!-- 圆角半径，可根据需要调整 -->
        </shape>

    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#8A96A4" /> <!-- 进度填充颜色，与滑块颜色一致 -->
                <corners android:radius="8px" /> <!-- 圆角半径，可根据需要调整 -->
            </shape>
        </clip>
    </item>
</layer-list>
