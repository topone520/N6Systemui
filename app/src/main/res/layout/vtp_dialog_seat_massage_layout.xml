<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1264px"
    android:layout_height="440px"
    android:layout_marginBottom="140px"
    android:background="@drawable/dialog_qs_bg"
    tools:ignore="PxUsage">

    <TextView
        android:id="@+id/pop_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="472px"
        android:layout_marginTop="40px"
        android:text="@string/seat_main_massage_mode"
        android:textColor="@color/hvac_title_color"
        android:textFontWeight="400"
        android:textSize="40px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.adayo.systemui.windows.views.seat.SeatMassageModeView
        android:id="@+id/seat_massage_rlv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="80px"
        android:layout_marginTop="120px"
        android:layout_marginEnd="80px" />
</RelativeLayout>