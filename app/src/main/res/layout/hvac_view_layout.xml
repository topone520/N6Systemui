<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/hvac_view_group"
    android:layout_width="3840px"
    android:layout_height="720px"
    android:background="@mipmap/ivi_comm_day_bg"
    android:paddingLeft="140px"
    android:paddingTop="66px">

    <RelativeLayout
        android:id="@+id/iv_hvac_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="-140px"
        android:layout_marginTop="-66px"
        android:background="@mipmap/ivi_ac_bg" />

    <View
        android:id="@+id/hvac_view_id1"
        android:layout_width="1000px"
        android:layout_height="match_parent"
        android:layout_marginTop="-66px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/hvac_view_id2"
        android:layout_width="1000px"
        android:layout_height="match_parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_hvac_seat"
        android:layout_width="160px"
        android:layout_height="68px"
        android:layout_marginLeft="100px"
        android:layout_marginTop="8px"
        android:background="@drawable/bg_hvac_btn_radius34"
        android:contentDescription="@string/hvac_seat"
        android:gravity="center"
        android:text="@string/hvac_seat"
        android:textColor="@color/text_color_normals"
        android:textSize="34px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_hvac_fragrance"
        android:layout_width="160px"
        android:layout_height="68px"
        android:layout_marginLeft="60px"
        android:layout_marginTop="8px"
        android:background="@drawable/bg_hvac_btn_radius34"
        android:contentDescription="@string/hvac_fragrance"
        android:gravity="center"
        android:text="@string/hvac_fragrance"
        android:textColor="@color/text_color_normals"
        android:textSize="34px"
        app:layout_constraintLeft_toRightOf="@+id/tv_hvac_seat"
        app:layout_constraintTop_toTopOf="parent" />

