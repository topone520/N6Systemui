<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1000px"
    android:layout_height="140px"
    tools:ignore="PxUsage,HardcodedText">
    <ImageView
        android:id="@+id/img_wifi_level"
        android:layout_width="56px"
        android:layout_height="56px"
        android:layout_marginStart="72px"
        android:layout_marginTop="32px"
        android:scaleType="center"
        android:src="@mipmap/ivi_top_icon_wifi_5_n">
    </ImageView>

    <TextView
        android:id="@+id/tv_wifi_name"
        android:layout_width="wrap_content"
        android:layout_height="48px"
        android:layout_marginStart="160px"
        android:layout_marginTop="36px"
        android:text="@string/screen_bluetooth_device_name"
        android:contentDescription="400"
        android:textColor="@color/hvac_title_color1"
        android:textSize="34px" />
    <ImageView
        android:id="@+id/img_wifi_disconnect"
        android:layout_marginStart="884px"
        android:layout_marginTop="32px"
        android:layout_width="56px"
        android:layout_height="56px"
        android:src="@mipmap/ivi_launcher_icon_lock_n"/>

</FrameLayout>