<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tool="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tool:background="@mipmap/vtp_ac_set_front_bg">

    <TextView
        android:id="@+id/tv_seat_pulse"
        android:layout_width="180px"
        android:layout_height="72px"
        android:layout_marginStart="525px"
        android:layout_marginTop="259px"
        android:background="@drawable/comm_c4972b8_bg"
        android:gravity="center"
        android:text="@string/seat_move1"
        android:textColor="@color/hvac_white"
        android:textSize="32px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.adayo.systemui.windows.views.seat.VtpSeatMassageImageView
        android:id="@+id/seat_massage"
        android:layout_width="110px"
        android:layout_height="110px"
        android:layout_marginStart="560px"
        android:layout_marginTop="379px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_seat_abdomen"
        android:layout_width="180px"
        android:layout_height="72px"
        android:layout_marginTop="259px"
        android:layout_marginEnd="525px"
        android:background="@drawable/comm_c8039414f_bg"
        android:gravity="center"
        android:text="@string/seat_move7"
        android:textColor="@color/white70"
        android:textSize="32px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.adayo.systemui.windows.views.seat.VtpSeatMassageImageView
        android:id="@+id/seat_abdomen"
        android:layout_width="110px"
        android:layout_height="110px"
        android:layout_marginTop="379px"
        android:layout_marginEnd="557px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_seat_abdomen"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>