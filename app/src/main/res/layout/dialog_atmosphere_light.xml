<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1926px"
    android:layout_height="656px"
    android:layout_marginBottom="40px"
    android:background="@drawable/comm_c2d3540_bg"
    tools:ignore="PxUsage">


    <RelativeLayout
        android:id="@+id/rl_dialog_title"
        android:layout_width="match_parent"
        android:layout_height="160px">

        <com.adayo.systemui.windows.views.SwitchButtonVe
            android:id="@+id/sb_light_switch"
            android:layout_width="72px"
            android:layout_height="44px"
            android:layout_centerVertical="true"
            android:layout_marginStart="60px"
            android:layout_marginTop="28px"
            android:background="@drawable/checkbox_selector"
            android:button="@null" />


        <TextView
            android:id="@+id/tv_title"
            android:layout_width="355px"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="172px"
            android:gravity="center_vertical"
            android:text="@string/screen_atmosphere"
            android:textColor="@color/hvac_title_color"
            android:textFontWeight="500"
            android:textSize="40px" />


        <TextView
            android:id="@+id/tv_more_setting"
            android:layout_width="136px"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="1654px"
            android:singleLine="true"
            android:text="@string/screen_bluetooth_more"
            android:textColor="@color/hvac_title_color1"
            android:textFontWeight="400"
            android:textSize="34px" />


        <ImageView
            android:id="@+id/iv_enter_more_setting"
            android:layout_width="48px"
            android:layout_height="48px"
            android:layout_centerVertical="true"
            android:layout_marginStart="1814px"
            android:src="@mipmap/ivi_menu_down_icon_pull_down_n" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="161px"
        android:background="@color/hvac_title_color1" />

    <TextView
        android:layout_width="288px"
        android:layout_height="44px"
        android:layout_marginStart="60px"
        android:layout_marginTop="212px"
        android:gravity="center_vertical"
        android:text="@string/atmosphere_light_rhythm_mode"
        android:textColor="@color/hvac_title_color1"
        android:textFontWeight="400"
        android:textSize="32px" />

    <RadioGroup
        android:id="@+id/rg_type"
        android:layout_width="880px"
        android:layout_height="100px"
        android:layout_marginStart="60px"
        android:layout_marginTop="280px"
        android:background="@drawable/seat_message_no_select_bg"
        android:orientation="horizontal">

        <RadioButton
            android:id="@+id/rb1"
            android:layout_width="220px"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:button="@null"
            android:gravity="center"
            android:text="@string/constant_display_mode"
            android:textColor="@drawable/comm_radiogroup_textview_color"
            android:background="@drawable/seat_massage_radiobutton"
            android:textSize="34px" />

        <RadioButton
            android:id="@+id/rb2"
            android:layout_width="220px"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:button="@null"
            android:gravity="center"
            android:text="@string/music_rhythm"
            android:textColor="@drawable/comm_radiogroup_textview_color"
            android:background="@drawable/seat_massage_radiobutton"
            android:textSize="34px" />

        <RadioButton
            android:id="@+id/rb3"
            android:layout_width="220px"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:button="@null"
            android:gravity="center"
            android:textColor="@drawable/comm_radiogroup_textview_color"
            android:background="@drawable/seat_massage_radiobutton"
            android:text="@string/driving_follow_up"
            android:textSize="34px" />

        <RadioButton
            android:id="@+id/rb4"
            android:layout_width="220px"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:button="@null"
            android:gravity="center"
            android:textColor="@drawable/comm_radiogroup_textview_color"
            android:background="@drawable/seat_massage_radiobutton"
            android:text="@string/interactive_follow_up"
            android:textSize="34px" />

    </RadioGroup>

    <TextView
        android:layout_width="288px"
        android:layout_height="44px"
        android:layout_marginStart="60px"
        android:layout_marginTop="428px"
        android:gravity="center_vertical"
        android:text="@string/atmosphere_light_brightness_adjustment"
        android:textColor="@color/hvac_title_color1"
        android:textFontWeight="400"
        android:textSize="32px" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="816px"
        android:layout_marginTop="428px"
        android:text="@string/automatic"
        android:textColor="@color/hvac_title_color"
        android:textFontWeight="400"
        android:textSize="32px" />

    <CheckBox
        android:id="@+id/checkbox_auto"
        android:layout_width="36px"
        android:layout_height="36px"
        android:layout_marginStart="904px"
        android:layout_marginTop="432px" />

    <com.adayo.systemui.windows.views.ScreenVolumeSeekBar
        android:id="@+id/sb_brightness_adjustment"
        android:layout_width="880px"
        android:layout_height="100px"
        android:layout_marginStart="60px"
        android:layout_marginTop="496px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/mediaText" />

    <TextView
        android:layout_width="288px"
        android:layout_height="44px"
        android:layout_marginStart="1040px"
        android:layout_marginTop="212px"
        android:gravity="center_vertical"
        android:text="@string/atmosphere_light_brightness_adjustment"
        android:textColor="@color/hvac_title_color1"
        android:textFontWeight="400"
        android:textSize="32px" />


    <SeekBar
        android:id="@+id/sb_set_color_bar"
        android:layout_width="826px"
        android:layout_height="100px"
        android:layout_marginStart="1040px"
        android:layout_marginTop="496px"
        android:background="@mipmap/ivi_setting_color_bar"
        android:maxHeight="32px"
        android:minHeight="32px"
        android:paddingLeft="0dp"
        android:progressDrawable="@null"
        android:thumb="@mipmap/vtp_setting_color_check_p_day"
        android:thumbOffset="0px" />


    <com.adayo.systemui.windows.views.GalleryRecyclerView
        android:id="@+id/rv_fwlightcolor"
        android:layout_width="826px"
        android:layout_height="165px"
        android:layout_marginStart="1040px"
        android:layout_marginTop="280px"
        android:orientation="horizontal" />

</FrameLayout>