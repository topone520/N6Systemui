            android:layout_height="wrap_content"
            android:layout_marginLeft="516px"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="436px"
                android:layout_marginTop="78px"
                android:layout_marginEnd="78px"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_rear"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    app:img_icon="@mipmap/ivi_menu_icon_rear_panel_n"
                    app:tv_title="@string/screen_rear" />

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_pedestrian"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_below="@+id/qs_rear"
                    android:layout_marginTop="56px"
                    app:img_icon="@mipmap/ivi_menu_icon_pedestrian_warning_n"
                    app:tv_title="@string/screen_pedestrian" />

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_slope"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_marginStart="56px"
                    android:layout_toEndOf="@+id/qs_rear"
                    app:img_icon="@mipmap/ivi_menu_icon_slope_n"
                    app:tv_title="@string/screen_slope" />

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_radar"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_below="@+id/qs_slope"
                    android:layout_marginStart="56px"
                    android:layout_marginTop="56px"
                    android:layout_toEndOf="@+id/qs_pedestrian"
                    app:img_icon="@mipmap/ivi_menu_icon_radar_n"
                    app:tv_title="@string/screen_radar" />

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_atmosphere"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_marginStart="56px"
                    android:layout_toEndOf="@+id/qs_slope"
                    app:img_icon="@mipmap/ivi_menu_icon_atmosphere_light_n"
                    app:tv_title="@string/screen_atmosphere" />

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_lock"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_below="@+id/qs_rear"
                    android:layout_marginStart="56px"
                    android:layout_marginTop="56px"
                    android:layout_toEndOf="@+id/qs_radar"
                    app:img_icon="@mipmap/ivi_menu_icon_ivi_lock_n"
                    app:tv_title="@string/screen_lock" />

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_child"
