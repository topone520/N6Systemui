                    android:layout_height="190px"
                    android:layout_marginStart="56px"
                    android:layout_toEndOf="@+id/qs_lock"
                    app:img_icon="@mipmap/ivi_menu_icon_child_lock_n"
                    app:tv_title="@string/screen_child" />


                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_skylight"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_below="@+id/qs_rear"
                    android:layout_marginStart="56px"
                    android:layout_marginTop="56px"
                    android:layout_toEndOf="@+id/qs_lock"
                    app:img_icon="@mipmap/ivi_menu_icon_skylight_n"
                    app:tv_title="@string/screen_skylight" />

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_sunroof"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_marginStart="56px"
                    android:layout_toEndOf="@+id/qs_child"
                    app:img_icon="@mipmap/ivi_menu_icon_skylight_n"
                    app:tv_title="@string/sunroof" />

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_cruise"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_below="@+id/qs_rear"
                    android:layout_marginStart="56px"
                    android:layout_marginTop="56px"
                    android:layout_toEndOf="@+id/qs_skylight"
                    app:img_icon="@mipmap/ivi_menu_icon_skylight_n"
                    app:tv_title="@string/intelligent_cruise" />

                <com.adayo.systemui.windows.views.QsIconView
                    android:id="@+id/qs_warning"
                    android:layout_width="240px"
                    android:layout_height="190px"
                    android:layout_marginStart="56px"
                    android:layout_toEndOf="@+id/qs_sunroof"
